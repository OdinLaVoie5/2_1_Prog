    "def check_conflicts(map_list, names=None):\n",
    "    \"\"\"\n",
    "    Проверяет список карт замен на циклы, цепочки, конфликты и дубли.\n",
    "    map_list : list[dict]\n",
    "    names    : список имён карт (по умолчанию MAP_1, MAP_2, ...)\n",
    "    \"\"\"\n",
    "\n",
    "    if names is None:\n",
    "        names = [f\"MAP_{i+1}\" for i in range(len(map_list))]\n",
    "\n",
    "    # --- Объединяем все пары (ключ → значение) в один общий список\n",
    "    all_pairs = []\n",
    "    for name, m in zip(names, map_list):\n",
    "        for k, v in m.items():\n",
    "            all_pairs.append((name, k, v))\n",
    "\n",
    "    # --- Строим граф замен\n",
    "    graph = {}\n",
    "    for _, k, v in all_pairs:\n",
    "        graph.setdefault(k, []).append(v)\n",
    "\n",
    "    # --- 1. Поиск циклов\n",
    "    cycles = []\n",
    "    def dfs(node, path):\n",
    "        if node not in graph:\n",
    "            return\n",
    "        for nxt in graph[node]:\n",
    "            if nxt in path:\n",
    "                cycles.append(path[path.index(nxt):] + [nxt])\n",
    "                continue\n",
    "            dfs(nxt, path + [nxt])\n",
    "\n",
    "    for n in graph:\n",
