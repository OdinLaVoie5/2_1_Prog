    "    for k in graph:\n",
    "        for v in graph[k]:\n",
    "            if v in graph and v != k:\n",
    "                chains.append((k, v, graph[v]))\n",
    "\n",
    "    # --- 3. Конфликты (одно значение у разных ключей)\n",
    "    rev = {}\n",
    "    for _, k, v in all_pairs:\n",
    "        rev.setdefault(v, set()).add(k)\n",
    "    conflicts = {v: ks for v, ks in rev.items() if len(ks) > 1}\n",
    "\n",
    "    # --- 4. Дубли (ключ в нескольких картах)\n",
    "    seen_keys = {}\n",
    "    for name, k, v in all_pairs:\n",
    "        seen_keys.setdefault(k, []).append(name)\n",
